include(@CMAKE_TOOLCHAIN_FILE@)
include(@CompilerFlagsCMAKE_FILE@)

SET(CMAKE_PREFIX_PATH @THIRDPARTY_PREFIX@ CACHE PATH "" FORCE)
SET(CMAKE_INSTALL_PREFIX @THIRDPARTY_PREFIX@ CACHE PATH "" FORCE)
SET(CMAKE_FIND_ROOT_PATH @CMAKE_FIND_ROOT_PATH@ CACHE PATH "" FORCE)

SET(PKG_CONFIG_PATH @THIRDPARTY_PKG_CONFIG_PATH@ CACHE PATH "" FORCE)
SET(PKG_CONFIG_LIBDIR @THIRDPARTY_PKG_CONFIG_LIBDIR@ CACHE PATH "" FORCE)
SET(PKG_CONFIG_EXECUTABLE @PKG_CONFIG_EXECUTABLE@ CACHE FILEPATH "" FORCE)

# Workaround until NDK-r23
# https://github.com/android/ndk/issues/929
SET(ZLIB_LIBRARY "@CMAKE_SYSROOT@/usr/lib/@CMAKE_LIBRARY_ARCHITECTURE@/@ANDROID_NATIVE_API_LEVEL@/libz.so")
SET(ZLIB_LIBRARY_RELEASE "@CMAKE_SYSROOT@/usr/lib/@CMAKE_LIBRARY_ARCHITECTURE@/@ANDROID_NATIVE_API_LEVEL@/libz.so")

