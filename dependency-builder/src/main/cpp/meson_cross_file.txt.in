
[binaries]
c = '@MESON_CC@'
cpp = '@MESON_CPP@'
ar = '@CMAKE_AR@'
ld = '@CMAKE_LINKER@'
strip = '@CMAKE_STRIP@'
pkgconfig = '@PKG_CONFIG_EXECUTABLE@'
llvm-config = '@MESON_LLVM-CONFIG@'
as = '@AS@'
nm = '@CMAKE_NM@'
objdump = '@CMAKE_OBJDUMP@'
ranlib = '@CMAKE_RANLIB@'

[properties]
needs_exe_wrapper = true
c_args='@CFLAGS@'
cpp_args='@CXXFLAGS@'
c_link_args='@LDFLAGS@'
cpp_link_args='@LDFLAGS@'

[host_machine]
system = '@MESON_SYSTEM@'
cpu_family = '@MESON_CPU_FAMILY@'
cpu = '@MESON_CPU@'
endian = '@MESON_ENDIAN@'

[paths]
prefix = '@THIRDPARTY_PREFIX@/'
libdir = 'lib/'
bindir = 'bin/'
